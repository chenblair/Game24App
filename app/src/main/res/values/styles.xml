<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="NumTile">
        <item name="android:layout_width">@dimen/num_tile_width</item>
        <item name="android:layout_height">@dimen/num_tile_height</item>
    </style>

    <style name="OpTile">
        <item name="android:layout_width">@dimen/op_tile_width</item>
        <item name="android:layout_height">@dimen/op_tile_height</item>
    </style>

</resources>
